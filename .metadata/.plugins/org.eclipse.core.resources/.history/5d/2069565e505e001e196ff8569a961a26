package controllers;

import org.hibernate.cfg.Configuration;

import com.mysql.cj.xdevapi.SessionFactory;

import models.Empleado;

public class EmpleadoController {
	
	public String createEmpleado(String nombres, String apellidos,String sexo){
		
		org.hibernate.SessionFactory sessionFactory = new Configuration().configure("hibernate.cfg.xml").addAnnotatedClass(Empleado.class).buildSessionFactory();
		return sexo;
		
				
		
	}

}
